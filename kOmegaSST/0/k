FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.639222044;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    "(topFlat|topSlope|bottomFlat1|bottomFlat2|bottomSlope|left)"
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    "(front|back)"
    {
        type            empty;
    }
}


